u0627\u062a\u06cc \u06a9\u0647 \u062f\u0631 \u062e\u0635\u0648\u0635 <strong>\u0628\u0647\u062a\u0631\u06cc\u0646 \u0631\u0648\u0627\u0646\u0634\u0646\u0627\u0633<\/strong> \u0648\u062c\u0648\u062f \u062f\u0627\u0631\u062f\u060c \u0644\u0627\u0632\u0645 \u0645\u06cc \u062f\u0627\u0646\u06cc\u0645 \u06cc\u06a9\u0633\u0631\u06cc \u062a\u0648\u0636\u06cc\u062d\u0627\u062a \u0628\u0647 \u0634\u0645\u0627 \u0639\u0632\u06cc\u0632\u0627\u0646 \u0627\u0631\u0627\u0626\u0647 \u0646\u0645\u0627\u06cc\u06cc\u0645. \u062d\u0648\u0632\u0647 \u0631\u0648\u0627\u0646\u0634\u0646\u0627\u0633\u06cc \u0628\u0633\u06cc\u0627\u0631 \u06af\u0633\u062a\u0631\u062f\u0647 \u0628\u0648\u062f\u0647 \u0648 \u06cc\u06a9 <strong>\u0631\u0648\u0627\u0646\u0634\u0646\u0627\u0633<\/strong> \u0646\u0645\u06cc \u062a\u0648\u0627\u0646\u062f \u062f\u0631 \u0647\u0645\u0647 \u0632\u0645\u06cc\u0646\u0647 \u0647\u0627 \u0645\u062a\u062e\u0635\u0635 \u0628\u0627\u0634\u062f. \u067e\u0633 \u062f\u0631 \u0648\u0647\u0644\u0647 \u0627\u0648\u0644 \u0645\u06cc \u0628\u0627\u06cc\u0633\u062a \u0628\u0631\u0627\u06cc \u062a\u0634\u062e\u06cc\u0635 \u0631\u0648\u0627\u0646\u0634\u0646\u0627\u0633 \u062e\u0648\u0628 \u0628\u0647 \u062d\u0648\u0632\u0647 \u062a\u062e\u0635\u0635\u06cc \u0631\u0648\u0627\u0646\u0634\u0646\u0627\u0633 \u062a\u0648\u062c\u0647 \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u06cc\u0645. \u062f\u0631 \u0627\u062f\u0627\u0645\u0647 \u0645\u06cc \u062a\u0648\u0627\u0646\u06cc\u0645 \u0631\u0632\u0648\u0645\u0647 \u06a9\u0627\u0631\u06cc \u0648 \u0647\u0645\u0686\u0646\u06cc\u0646 \u0628\u0627\u0632\u062e\u0648\u0631\u062f\u0647\u0627\u06cc \u0645\u062b\u0628\u062a \u0648 \u0645\u0646\u0641\u06cc \u0645\u0631\u0627\u062c\u0639\u06cc\u0646 \u0631\u0627 \u0645\u0648\u0631\u062f \u0627\u0631\u0632\u06cc\u0627\u0628\u06cc \u0642\u0631\u0627\u0631 \u062f\u0647\u06cc\u0645 \u062a\u0627 \u0628\u0647\u062a\u0631\u06cc\u0646 \u0631\u0648\u0627\u0646\u0634\u0646\u0627\u0633 \u06a9\u0647 \u062f\u0631 \u06cc\u06a9 \u06cc\u0627 \u0686\u0646\u062f \u062d\u0648\u0632\u0647 \u062a\u062e\u0635\u0635 \u062f\u0627\u0631\u062f \u0631\u0627 \u0628\u0631\u0627\u06cc \u0645\u0634\u0627\u0648\u0631\u0647 \u0648 \u062f\u0631\u0645\u0627\u0646 \u062e\u0648\u062f \u0627\u0646\u062a\u062e\u0627\u0628 \u06a9\u0646\u06cc\u0645. \u062a\u0648\u062c\u0647 \u0628\u0647 \u0627\u06cc\u0646 \u0646\u06a9\u062a\u0647 \u0646\u06cc\u0632 \u0636\u0631\u0648\u0631\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0628\u0647\u062a\u0631\u06cc\u0646 \u0631\u0648\u0627\u0646\u0634\u0646\u0627\u0633 \u062f\u0646\u06cc\u0627 \u0646\u06cc\u0632 \u0628\u062f\u0648\u0646 \u0647\u0645\u06a9\u0627\u0631\u06cc \u0648 \u0627\u0631\u0627\u062f\u0647 \u0634\u062e\u0635 \u0628\u0631\u0627\u06cc \u062d\u0644 \u0648 \u062f\u0631\u0645\u0627\u0646 \u0645\u0634\u06a9\u0644\u0627\u062a\u060c \u0646\u0645\u06cc \u062a\u0648\u0627\u0646\u062f \u06a9\u0627\u0631 \u0642\u0627\u0628\u0644 \u062a\u0648\u062c\u0647\u06cc \u0627\u0646\u062c\u0627\u0645 \u062f\u0647\u062f. \u0628\u0646\u0627\u0628\u0631\u0627\u06cc\u0646 \u062d\u062a\u0645\u0627 \u0628\u0627\u06cc\u062f \u062f\u0631 \u0646\u0638\u0631 \u062f\u0627\u0634\u062a \u06a9\u0647 \u0647\u0645\u06a9\u0627\u0631\u06cc \u0648 \u0635\u062f\u0627\u0642\u062a \u0628\u0627 \u0645\u0634\u0627\u0648\u0631 \u062f\u0631\u0645\u0627\u0646\u06af\u0631\u060c \u06cc\u06a9 \u0627\u0635\u0644 \u0627\u062c\u062a\u0646\u0627\u0628 \u0646\u0627\u067e\u0630\u06cc\u0631 \u0648 \u06cc\u06a9 \u0642\u062f\u0645 \u0628\u0632\u0631\u06af \u0628\u0631\u0627\u06cc \u062d\u0644 \u0645\u0634\u06a9\u0644\u0627\u062a \u0627\u0633\u062a.<\/p>\n<\/div><\/div><\/div><\/div><\/div><\/div><\/div><\/div><\/div><\/div><\/div><\/section><section class=\"l-section wpb_row us_custom_d629ee44 height_small\"><div class=\"l-section-h i-cf\"><div class=\"g-cols vc_row type_default valign_top\"><div class=\"vc_col-sm-12 wpb_column vc_column_container\"><div class=\"vc_column-inner\"><div class=\"wpb_wrapper\"><div class=\"w-separator size_medium with_line width_default thick_1 style_solid color_border align_center with_content\"><div class=\"w-separator-h\"><h3 class=\"w-separator-text\"><span>\u0648\u06cc\u0698\u06af\u06cc \u0647\u0627\u06cc \u06cc\u06a9 \u0631\u0648\u0627\u0646\u0634\u0646\u0627\u0633 \u062e\u0648\u0628<\/span><\/h3><\/div><\/div><div class=\"w-separator size_custom\" style=\"height:20px\"><\/div><div class=\"g-cols wpb_row  type_default valign_top vc_inner\"><div class=\"vc_col-sm-6 wpb_column vc_column_container\"><div class=\"vc_column-inner\"><div class=\"wpb_wrapper\"><div class=\"wpb_text_column\" ><div class=\"wpb_wrapper\"><ul>\n<li>\u0628\u0631\u062e\u0648\u0631\u062f \u0645\u062a\u0646\u0627\u0633\u0628 \u0628\u0627 \u0627\u0641\u0631\u0627\u062f \u0648 \u062f\u0631 \u0646\u0638\u0631 \u06af\u0631\u0641\u062a\u0646 \u0634\u0631\u0627\u06cc\u0637 \u0647\u0631 \u0634\u062e\u0635<\/li>\n<li><span style=\"color: #e95095;\">\u06af\u0634\u0627\u062f\u0647 \u0631\u0648\u06cc\u06cc\u060c \u067e\u0630\u06cc\u0631\u0627 \u0628\u0648\u062f\u0646 \u0648 \u0645\u0647\u0631\u0628\u0627\u0646\u06cc<\/span><\/li>\n<li>\u062a\u0644\u0627\u0634 \u0628\u0631\u0627\u06cc \u0628\u0647\u0628\u0648\u062f \u0627\u0631\u062a\u0628\u0627\u0637\u0627\u062a \u0645\u06cc\u0627\u0646 \u0627\u0641\u0631\u0627\u062f \u0628\u062e\u0635\u0648\u0635 \u062e\u0627\u0646\u0648\u0627\u062f\u0647 \u0648 \u0632\u0648\u062c<\/li>\n<li><span style=\"color: #e95095;\">\u062a\u0644\u0627\u0634 \u0628\u0631\u0627\u06cc \u0627\u0635\u0644\u0627\u062d \u06a9\u0631\u062f\u0646 \u0631\u0641\u062a\u0627\u0631\u0647\u0627\u06cc \u0646\u0627\u062f\u0631\u0633\u062a \u0645\u0631\u0627\u062c\u0639\u06cc\u0646<\/span><\/li>\n<li>\u062f\u0627\u0634\u062a\u0646 \u0645\u0647\u0627\u0631\u062a \u0627\u0631\u062a\u0628\u0627\u0637\u06cc \u0628\u0627\u0644\u0627 \u0628\u0631\u0627\u06cc \u0628\u0631\u0642\u0631\u0627\u0631\u06cc \u0627\u0631\u062a\u0628\u0627\u0637 \u062f\u0631\u0645\u0627\u0646\u06cc<\/li>\n<li><span style=\"color: #e95095;\">\u062f\u0627\u0634\u062a\u0646 \u0636\u0631\u06cc\u0628 \u0647\u0648\u0634\u06cc \u0628\u0627\u0644\u0627 \u0648 \u062a\u0648\u062c\u0647 \u0628\u0647 \u0646\u06a9\u0627\u062a \u0631\u06cc\u0632 \u0648 \u062c\u0632\u0626\u06cc\u0627\u062a<\/span><\/li>\n<li>\u062f\u0627\u0634\u062a\u0646 \u0627\u0646\u0631\u0698\u06cc \u0628\u0627\u0644\u0627 \u0648 \u0627\u0646\u062a\u0642\u0627\u0644 \u0627\u06cc\u0646 \u0627\u0646\u0631\u0698\u06cc \u0628\u0647 \u0645\u0631\u0627\u062c\u0639\u06cc\u0646<\/li>\n<li><span style=\"color: #e95095;\">\u0646\u0633\u062e\u0647 \u0627\u06cc \u06a9\u0647 \u0628\u0631\u0627\u06cc \u062f\u06cc\u06af\u0631\u0627\u0646 \u0645\u06cc \u067e\u06cc\u0686\u06cc\u062f \u0628\u0631\u0627\u06cc \u062e\u0648\u062f\u0634 \u0647\u0645 \u0635\u062f\u0642 \u06a9\u0646\u062f.<\/span><\/li>\n<li>\u062f\u0631\u06a9 \u0634\u0631\u0627\u06cc\u0637 \u0645\u0631\u0627\u062c\u0639\u0647 \u06a9\u0646\u0646\u062f\u0647 \u0648 \u0642\u0631\u0627\u0631 \u062f\u0627\u062f\u0646 \u062e\u0648\u062f \u062f\u0631 \u0622\u0646 \u0634\u0631\u0627\u06cc\u0637<\/li>\n<li><span style=\"color: #e95095;\">\u0628\u0627\u06cc\u062f \u0642\u0627\u0628\u0644 \u0627\u0639\u062a\u0645\u0627\u062f \u0628\u0648\u062f\u0647 \u0648 \u0627\u0645\u06cc\u0646 \u0627\u0633\u0631\u0627\u0631 \u0645\u0631\u0627\u062c\u0639\u06cc\u0646 \u062e\u0648\u062f \u0628\u0627\u0634\u062f.<\/span><\/li>\n<li>\u0628\u0627\u06cc\u062f \u062e\u0648\u0634 \u0638\u0627\u0647\u0631 \u0628\u0648\u062f\u0647 \u0648 \u0634\u062e\u0635\u06cc \u062c\u0630\u0627\u0628 \u0648 \u0628\u0627\u0646\u0641\u0648\u0630 \u0628\u0627\u0634\u062f.<\/li>\n<li><span style=\"color: #e95095;\">\u0647\u0645\u0648\u0627\u0631\u0647 \u0627\u062e\u0644\u0627\u0642 \u062d\u0631\u0641\u0647 \u0627\u06cc \u0631\u0627 \u0631\u0639\u0627\u06cc\u062a \u06a9\u0631\u062f\u0647 \u0648 \u0645\u0646\u0635\u0641 \u0648 \u0628\u0627\u0648\u062c\u062f\u0627\u0646 \u0628\u0627\u0634\u062f.<\/span><\/li>\n<li>\u0627\u06af\u0631 \u062c\u0648\u0627\u0628 \u0633\u0648\u0627\u0644\u06cc \u0631\u0627 \u0646\u062f\u0627\u0646\u062f \u0628\u0647 \u0631\u0627\u062d\u062a\u06cc \u0628\u06af\u0648\u06cc\u062f \u0646\u0645\u06cc\u062f\u0627\u0646\u0645!<\/li>\n<li><span style=\"color: #e95095;\">\u062f\u0631 \u0631\u0634\u062a\u0647 \u0648 \u062a\u062e\u0635\u0635 \u062e\u0648\u062f \u062f\u0627\u0646\u0627 \u0648 \u0645\u062a\u062e\u0635\u0635 \u062a\u0645\u0627\u0645 \u0639\u06cc\u0627\u0631 \u0628\u0627\u0634\u062f.<\/span><\/li>\n<\/ul>\n<\/div><\/div><\/div><\/div><\/div><div class=\"vc_col-sm-6 wpb_column vc_column_container\"><div class=\"vc_column-inner\"><div class=\"wpb_wrapper\"><div class=\"wpb_text_column\" ><div class=\"wpb_wrapper\"><ul>\n<li>\u06a9\u0644 \u0632\u0645\u0627\u0646 \u0645\u0634\u0627\u0648\u0631\u0647 \u0631\u0627 \u0628\u0647 \u0645\u0631\u0627\u062c\u0639 \u0627\u062e\u062a\u0635\u0627\u0635 \u062f\u0647\u062f.<\/li>\n<li><span style=\"color: #e95095;\">\u0628\u0627 \u0645\u0631\u0627\u062c\u0639\u0634 \u0648\u0627\u0631\u062f \u0631\u0627\u0628\u0637\u0647 \u062e\u0635\u0648\u0635\u06cc \u0648 \u063a\u06cc\u0631\u0645\u062a\u0639\u0627\u0631\u0641 \u0646\u0634\u0648\u062f.<\/span><\/li>\n<li>\u0648\u06cc\u0698\u06af\u06cc\u0647\u0627\u06cc \u0627\u062e\u0644\u0627\u0642\u06cc \u0631\u0627 \u0628\u0635\u0648\u0631\u062a \u06a9\u0627\u0645\u0644 \u0631\u0639\u0627\u06cc\u062a \u0646\u0645\u0627\u06cc\u062f.<\/li>\n<li><span style=\"color: #e95095;\">\u062a\u0631\u0628\u06cc\u062a \u0645\u0639\u0646\u0648\u06cc \u0631\u0627 \u062f\u0631 \u0646\u0638\u0631 \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u062f.<\/span><\/li>\n<li>\u0631\u0648\u0627\u0646\u0634\u0646\u0627\u0633 \u062e\u0648\u0628 \u0628\u0627\u06cc\u062f \u0628\u0647 \u0632\u0628\u0627\u0646 \u0628\u062f\u0646 \u0645\u0633\u0644\u0637 \u0628\u0627\u0634\u062f.<\/li>\n<li><span style=\"color: #e95095;\">\u0631\u0648\u0627\u0646\u0634\u0646\u0627\u0633 \u062e\u0648\u0628 \u0628\u0627\u06cc\u062f \u0627\u0639\u062a\u0645\u0627\u062f \u0628\u0647 \u0646\u0641\u0633 \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u062f.<\/span><\/li>\n<li>\u0628\u0627\u06cc\u062f \u0628\u0647 \u062f\u0648\u0631 \u0627\u0632 \u0627\u0633\u062a\u0631\u0633 \u0628\u0648\u062f\u0647 \u0648 \u0622\u0631\u0627\u0645\u0634 \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u062f.<\/li>\n<li><span style=\"color: #e95095;\">\u0628\u0635\u0648\u0631\u062a \u06a9\u0627\u0645\u0644 \u0628\u0647 \u062d\u0631\u0641 \u0647\u0627\u06cc \u0645\u0631\u0627\u062c\u0639 \u062a\u0648\u062c\u0647 \u06a9\u0646\u062f.<\/span><\/li>\n<li>\u0633\u0639\u06cc \u06a9\u0646\u062f \u0627\u0633\u0645 \u0645\u0631\u0627\u062c\u0639\u06cc\u0646 \u0631\u0627 \u0628\u0647 \u062e\u0627\u0637\u0631 \u0628\u0633\u067e\u0627\u0631\u062f.<\/li>\n<li><span style=\"color: #e95095;\">\u062f\u0631 \u0628\u062f\u062a\u0631\u06cc\u0646 \u0634\u0631\u0627\u06cc\u0637 \u0628\u0647 \u0645\u0631\u0627\u062c\u0639\u06cc\u0646 \u062e\u0648\u062f \u0627\u0645\u06cc\u062f \u0648 \u0627\u0646\u06af\u06cc\u0632\u0647 \u0628\u062f\u0647\u062f.<\/span><\/li>\n<li>\u062f\u0631 \u067e\u0627\u06cc\u0627\u0646 \u0647\u0631 \u062c\u0644\u0633\u0647\u060c \u0631\u0627\u0647\u06a9\u0627\u0631\u06cc \u0645\u062a\u0646\u0627\u0633\u0628 \u0627\u0631\u0627\u0626\u0647 \u062f\u0647\u062f.<\/li>\n<li><span style=\"color: #e95095;\">\u06a9\u0648\u062f\u06a9 \u062f\u0631\u0648\u0646 \u0641\u0639\u0627\u0644\u06cc \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u062f.<\/span><\/li>\n<li>\u0627\u0632 \u0634\u06cc\u0648\u0647 \u062f\u0631\u0645\u0627\u0646\u06cc \u0645\u062a\u0646\u0627\u0633\u0628 \u0628\u0627 \u0645\u0631\u0627\u062c\u0639\u0647 \u06a9\u0646\u0646\u062f\u0647 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u06a9\u0646\u062f.<\/li>\n<li><span style=\"color: #e95095;\">\u0627\u062d\u062a\u0631\u0627\u0645 \u0628\u0647 \u0647\u0645\u0647 \u0645\u0631\u0627\u062c\u0639\u06cc\u0646 \u0648 \u0631\u0639\u0627\u06cc\u062a \u0639\u062f